<div class="quarterly-dashboard">
  <div class="quarterly-events">
    {% if event_no is empty  %}
      <p>No records found!</p>
    {% else %}
      <table class="table table-striped table-hover">
        <thead class="table-primary">
          <tr>
            <th>Event Qtr</th>
            <th>Number of Events</th>
          </tr>
        </thead> 
        <tbody> 
        {% for i in range(0, quarters|length) %} 
          <tr>
            <td>{{ quarters[i] }}</td>
            <td>{{ event_no[i] }}</td>
          </tr>
        {% endfor %}
        </tbody>
      </table>
    {% endif %}
  </div>
</div>
